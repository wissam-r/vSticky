<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
     </style> 
    

	<style name="MyListView">
	  <item name="android:onClick">
		<selector xmlns:android="http://schemas.android.com/apk/res/android">
  		<item android:state_enabled="false" android:color="#333" />
  		<item android:state_window_focused="false" android:color="#CCC" />
  		<item android:state_pressed="true" android:color="#FFF" />
  		<item android:state_selected="true" android:color="#FFF" />
	</selector>
	      </item>

	</style>
	<style name="MyListView1" parent="@android:style/Widget.DeviceDefault.Light">
	  <item name="android:background">@android:color/holo_blue_bright</item>
	</style>
         
    

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
   		
	</style>

</resources>
